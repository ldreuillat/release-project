{
  "_all": {
    "enabled": false
  },
  "dynamic_templates": [
    {
      "no_thumbnail_template": {
        "mapping": {
          "index": false
        },
        "path_match": "thumb:thumbnail.*"
      }
    },
    {
      "no_picture_template": {
        "mapping": {
          "index": false
        },
        "path_match": "picture:views.*"
      }
    },
    {
      "strings": {
        "mapping": {
          "type": "keyword",
          "copy_to": "all_field",
          "ignore_above": 256
        },
        "match_mapping_type": "string"
      }
    }
  ],
  "properties": {
    "all_field": {
      "type": "text",
      "analyzer": "fulltext"
    },
    "ecm:name": {
      "type": "keyword",
      "ignore_above": 256,
      "copy_to": "all_field"
    },
    "ecm:acl": {
      "type": "keyword"
    },
    "ecm:binarytext": {
      "type": "text",
      "analyzer": "fulltext",
      "copy_to": "all_field"
    },
    "ecm:path": {
      "fields": {
        "children": {
          "type": "text",
          "search_analyzer": "keyword",
          "analyzer": "path_analyzer"
        }
      },
      "type": "keyword"
    },
    "ecm:pos": {
      "type": "integer"
    },
    "ecm:isCheckedIn": {
      "type": "boolean"
    },
    "ecm:isLatestMajorVersion": {
      "type": "boolean"
    },
    "ecm:isLatestVersion": {
      "type": "boolean"
    },
    "ecm:isProxy": {
      "type": "boolean"
    },
    "ecm:isVersion": {
      "type": "boolean"
    },
    "common:icon": {
      "index": false,
      "type": "keyword"
    },
    "dc:title": {
      "type": "text",
      "copy_to": "all_field",
      "fielddata": true,
      "fields": {
        "fulltext": {
          "analyzer": "fulltext",
          "type": "text",
          "boost": 2
        },
        "lower_case_sort": {
          "type":     "text",
          "analyzer": "lowercase_analyzer"
        }
      }
    },
    "dc:description": {
      "type": "keyword",
      "copy_to": "all_field",
      "ignore_above": 32765,
      "fields": {
        "fulltext": {
          "analyzer": "fulltext",
          "type": "text",
          "boost": 1.5
        }
      }
    },
    "dc:created": {
      "type": "date"
    },
    "dc:expired": {
      "type": "date"
    },
    "dc:modified": {
      "type": "date"
    },
    "dc:creator": {
      "type": "keyword",
      "ignore_above": 256
    },
    "dc:lastContributor": {
      "type": "keyword",
      "ignore_above": 256
    },
    "ecm:uuid": {
      "type": "keyword",
      "ignore_above": 256
    },
    "ecm:mixinType": {
      "type": "keyword",
      "ignore_above": 256
    },
    "ecm:currentLifeCycleState": {
      "type": "keyword",
      "ignore_above": 256
    },
    "ecm:versionVersionableId": {
      "type": "keyword",
      "ignore_above": 256
    },
    "ecm:primaryType": {
      "type": "keyword",
      "ignore_above": 256
    },
    "note:note": {
      "type": "keyword",
      "copy_to": "all_field",
      "ignore_above": 256,
      "fields": {
        "fulltext": {
          "analyzer": "fulltext",
          "type": "text"
